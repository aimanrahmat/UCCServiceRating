<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.aimanrahmat.uccservicerating">

    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".PageMain"
            android:label="@string/app_name" />
        <activity
            android:name=".PageRating"
            android:label="@string/title2" />
        <activity
            android:name=".PageEnd"
            android:label="@string/title3" />
        <activity
            android:name=".PageInfo"
            android:label="@string/title4" />
        <activity
            android:name=".PageAbout"
            android:label="@string/title6" />
        <activity
            android:name=".PageStats"
            android:label="@string/title_activity_page_stats"
            android:theme="@style/AppTheme"></activity>
    </application>

</manifest>